bool isAnagram(string s, string t) {
        unordered_map<char,int>mp1;
        unordered_map<char,int>mp2;
        if(s.length()!=t.length())
        return false;
        for(auto it:s)
        {
            mp1[it]++;
        }
        for(auto it:t)
        {
            mp2[it]++;
        }
       for(auto it:s)
       {
           if(mp1[it]!=mp2[it])
           return false;

       } 
    return true;
    }
