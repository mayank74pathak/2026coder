bool isValid(string s) {
        stack<char>st;
        for(auto c:s)
        {
            if(c=='{' or c=='[' or c=='(')
            st.push(c);

            else if(st.empty() or (st.top()=='{' and c!='}') or (st.top()=='(' and c!=')') or (st.top()=='[' and c!=']'))
               return false;
            else
            st.pop();   
        }
    if(st.empty())
      return true;
      return false;
    }
