class Solution {
public:
    bool isIsomorphic(string s, string t) {
        if (s.size()!=t.size())
        {
            return false;
        }
       int m[256]={0};
       int n[256]={0};
       for (int i=0;i<s.size();i++)
       {
        if(!m[s[i]]and !n[t[i]])
        {
            m[s[i]]=t[i];
            n[t[i]]=s[i];
        }
       else if (m[s[i]]!=t[i])
       {
           return false;
       }
       }
    return true;
    }
};
