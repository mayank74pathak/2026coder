class Solution {
  public:
    void insertionSort(vector<int>& arr) {
        // code here
        for(int i=0;i<arr.size()-1;i++)
        {
            for(int j=i;j>=0;j--)
            {
                if(arr[j]>arr[j+1])
                {
                int t;
                t=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=t;
                }
            else{
                break;
            }
                
            }
        }
    }
};
