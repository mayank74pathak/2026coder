ListNode* reverseList(ListNode* head) {
        if(head==NULL or head->next==NULL)
        return head;
        ListNode*curr=head;
        ListNode*prev=NULL;
        ListNode*front=head->next;
        while(front!=NULL)
        {
           curr->next=prev;
           prev=curr;
           curr=front;
           front=front->next;
        }
        curr->next=prev;
        return curr;
    }
