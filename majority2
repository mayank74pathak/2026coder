vector<int> majorityElement(vector<int>& nums) {
        vector<int>ans;
        int count=nums.size()/3;
        unordered_map<int,int>mapp;
        for(int i=0;i<nums.size();i++)
        {
            mapp[nums[i]]++;
        }
       for(auto n:mapp)
       {
        if(n.second>count)
        ans.push_back(n.first);
       }
    return ans;
    }
