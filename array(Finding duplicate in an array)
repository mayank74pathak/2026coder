class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        unordered_map<int,int>mp;
        for(auto it:nums)
        {
            mp[it]++;
        }
     for(auto it:mp)// if you want to apply first and second then you must use map in loop like mp 
     {
        if(it.second>=2)
        return true;
    
     }
    return false;
    }
};
